<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu"
    android:background="?attr/colorBackgroundFloating">

    <ImageView
        android:id="@+id/barraSuperiorImageView"
        android:layout_width="match_parent"
        android:layout_height="51dp"
        android:background="@color/colorBarraSuperior"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="DATE / HOUR"
        android:textSize="20dp"
        android:textColor="@color/colorFondo"
        app:layout_constraintEnd_toEndOf="@+id/barraSuperiorImageView"
        app:layout_constraintStart_toStartOf="@+id/barraSuperiorImageView"
        app:layout_constraintTop_toTopOf="@+id/barraSuperiorImageView" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Date/Time"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/barraSuperiorImageView"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="@+id/barraSuperiorImageView"
        app:layout_constraintTop_toBottomOf="@+id/barraSuperiorImageView" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="132dp"
        android:text="Sunrise"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/barraSuperiorImageView"
        app:layout_constraintHorizontal_bias="0.081"
        app:layout_constraintStart_toStartOf="@+id/barraSuperiorImageView"
        app:layout_constraintTop_toBottomOf="@+id/barraSuperiorImageView" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="368dp"
        android:text="Enter Time Zone"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/barraSuperiorImageView"
        app:layout_constraintStart_toStartOf="@+id/barraSuperiorImageView"
        app:layout_constraintTop_toBottomOf="@+id/barraSuperiorImageView" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="44dp"
        android:text="Sunset"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeZone_TextView" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Time Zone"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/barraSuperiorImageView"
        app:layout_constraintHorizontal_bias="0.909"
        app:layout_constraintStart_toStartOf="@+id/barraSuperiorImageView"
        app:layout_constraintTop_toBottomOf="@+id/barraSuperiorImageView" />

    <TextView
        android:id="@+id/set_time_date_TextView"
        android:layout_width="142dp"
        android:layout_height="54dp"
        android:layout_marginTop="8dp"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/barraSuperiorImageView"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toStartOf="@+id/barraSuperiorImageView"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/sunrise_TextView"
        android:layout_width="142dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/validate_textView"
        android:layout_width="317dp"
        android:layout_height="61dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="292dp"
        android:textAlignment="center"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/sunset_TextView"
        android:layout_width="142dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="4dp"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <TextView
        android:id="@+id/timeZone_TextView"
        android:layout_width="132dp"
        android:layout_height="54dp"
        android:layout_marginTop="8dp"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/barraSuperiorImageView"
        app:layout_constraintHorizontal_bias="0.938"
        app:layout_constraintStart_toStartOf="@+id/barraSuperiorImageView"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <EditText
        android:id="@+id/timezone_EditText"
        android:layout_width="142dp"
        android:layout_height="45dp"
        android:layout_marginStart="48dp"
        android:textColor="?attr/editTextColor"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <Button
        android:id="@+id/button_timezone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="52dp"
        android:text="Validate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeZone_TextView"
        app:srcCompat="@drawable/sunrise" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sunrise_TextView"
        app:srcCompat="@drawable/sunset" />


</androidx.constraintlayout.widget.ConstraintLayout>